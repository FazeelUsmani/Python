import turtle

turtle.clear()
turtle.forward(120)
turtle.penup()
turtle.right(90)
turtle.forward(40)
turtle.pendown()
turtle.right(90)
turtle.forward(120)

turtle.penup()
turtle.goto(40, 40)
turtle.pendown()
turtle.left(90)
turtle.forward(120)
turtle.left(90)
turtle.penup()
turtle.forward(40)
turtle.left(90)
turtle.pendown()
turtle.forward(120)



turtle.pencolor("green")



